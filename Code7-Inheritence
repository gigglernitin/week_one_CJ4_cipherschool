package CoreJava.Week1code1CipherSchool;
class A{
	int a,b;
	
	public void show() {
		System.out.println("Hello!");
	}
}
interface b{
	public void eat();
}

interface D extends b{
	public void sleep();
}
class C implements b,D{
	int e,f;
	public void speak() {
		System.out.println("Speaking");
	}
	
	@Override
	public void eat() {
		
	}
	
	@Override
	public void sleep() {
		
	}
}
